{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDb)\\MSSQLLocalDB;Database=Bank6; Integrated Security=True; TrustServerCertificate=True;"
  },
  "Jwt": {
    "Issuer": "http://localhost:7221",
    "Audience": "http://localhost:4200",
    "EXPIRATION_MINUTES": 60,
    "Key": ""
  },
  "ExchangeRateApi": {
    "BaseUrl": "https://nbg.gov.ge/gw/api/ct/monetarypolicy/currencies/ka/json/?currencies=",
    "CacheExpirationMinutes": 60
  },
  "Security": {
    "CardEncryptionKey": "A91B5ED452CF8C7A4DCE36B95B922"
  },
  "Atm": {
    "FeeRate": 0.02,
    "DailyWithdrawalLimit": 10000
  },
  "Transaction": {
    "TransferToOthersFeeRate": 0.01,
    "TransferToOthersBaseFee": 0.5,
    "BaseCurrency": "GEL"
